#include "holberton.h"
/**
 * print_chessboard - Entry point
 * Description: Function that return the chessboard
 * @a: parametre to check
 * Return: unsigned
 */
void print_chessboard(char (*a)[8])
{
	int i;
	int j = 0;

	for (i = 0 ; a[i] ; i++)
	{
		for (j = 0; a[j]; j++)
			_putchar(a[i][j]);
		_putchar('\n');

	}
}
