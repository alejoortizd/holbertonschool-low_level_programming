#include <stdio.h>
#include <stdlib.h>
#include <stdarg.h>
#include "variadic_functions.h"
/**
 *
 *
 */
void pchar(va_list l)
{
	printf("%c", va_arg(l, char));
}
/**
 *
 *
 */
void pint(va_list l)
{
        printf("%d", va_arg(l, int));
}
/**
 *
 *
 */
void pflo(va_list l)
{
	printf("%f", va_arg(l, float));
}
/**
 *
 *
 */
void pstr(va_list l)
{
	printf("%s", va_arg(l, char *));
}
