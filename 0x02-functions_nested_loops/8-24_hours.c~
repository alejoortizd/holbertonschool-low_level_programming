#include "holberton.h"
/**
 * jack_bauer - Entry point
 *
 * @void: parametre to check
 *
 */
void jack_bauer(void)
{
	int a = 0;
	int b = 0;

	for ( ; a <= 23; a++)
	{
		for ( ; b <= 59; b++)
		{
			putchar((a / 10) + '0');
			putchar((a % 10) + '0');
			putchar(':');
			putchar((b / 10) + '0');
			putchar((b % 10) + '0');
			putchar('\n');
		}
		b = 0;
	}
}
